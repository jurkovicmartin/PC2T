package ProjectFilms;

import java.util.ArrayList;
import java.util.List;

public class Program {

	public static void main(String[] args) {
		
		FeatureFilm Transformers = new FeatureFilm("Transformers", "Michael Bay", 2000);
		Films.addFilm(Transformers);
		
		List<String> actors = new ArrayList<String>();
		actors.add("Silvester Stalone");
		actors.add("Micheal B Jordan");
		FeatureFilm Creed = new FeatureFilm("Creed", "Rony", 2014, actors);
		
		System.out.println(Transformers);
		System.out.println(Creed);
		
		Creed.editDirector("Martyn");
	}

}
